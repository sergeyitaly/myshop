{
	"compilerOptions": {
	  "target": "es2021",
	  "lib": ["es2021"],
	  "module": "ESNext",
	  "moduleResolution": "bundler",  // Fix: Use 'bundler' instead of 'Bundler'
	  "types": ["@cloudflare/workers-types", "node"],
	  "resolveJsonModule": true,
	  "noEmit": false,
	  "outDir": "./dist",
	  "strict": true,
	  "skipLibCheck": true
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules"]
  }
  