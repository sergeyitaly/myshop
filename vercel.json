
{
  "version": 2,
  "builds": [
    {
      "src": "myshop/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "150mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myshop/wsgi.py"
    },    
    {
      "src": "/dist/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/assets/$1"
    },
  ]
}