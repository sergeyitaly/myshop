{
  "version": 2,
  "builds": [
    {
      "src": "myshop/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myshop/wsgi.py"
    },
    {
      "src": "/dist/(.*)",
      "dest": "https://kolorytmedia.s3.amazonaws.com/dist/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "https://kolorytmedia.s3.amazonaws.com/dist/assets/$1"
    },
    {
      "src": "/templates/(.*)",
      "dest": "https://kolorytmedia.s3.amazonaws.com/templates/$1"
    }
  ]
}
