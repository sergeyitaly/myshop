{
  "version": 2,
  "builds": [
    {
      "src": "myshop/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "150mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myshop/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}
