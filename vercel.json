{
  "version": 2,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "https://kolorytmedia.s3.amazonaws.com/staticfiles_build/static/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "https://kolorytmedia.s3.amazonaws.com/staticfiles_build/static/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myshop/wsgi.py"
    }
  ],
  "functions": {
    "build": {
      "runtime": "@vercel/build-utils",
      "includeFiles": "build_files.sh"
    }
  }
}
