{
  "version": 2,
  "builds": [
    {
      "src": "myshop/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/dist/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },    
    {
      "src": "/templates/(.*)",
      "dest": "/templates/$1"
    }, 
    {
      "src": "/(.*)",
      "dest": "myshop/wsgi.py"
    }

  ]
}
