{
  "version": 2,
  "name": "koloryt",
  "builds": [
    {
      "src": "myshop/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "120mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "static"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myshop/wsgi.py"
    }
  ]
}