{
  "version": 2,
  "builds": [
    {
      "src": "myshop/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "120mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
        "src": "/(.*)",
        "dest": "/",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "PATCH",
            "DELETE",
            "OPTIONS"
        ]
    }
  ]
}
